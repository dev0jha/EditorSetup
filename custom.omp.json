{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#d7db20",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#5e3ce3",
          "options": {
            "style": "folder"
          },
          "style": "plain",
          "template": "  {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#2ee124",
          "options": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#1F4CE0>git:</>({{ .HEAD }})</>",
          "type": "git"
        },
        {
          "foreground": "#be26a8",
          "style": "plain",
          "template": " \u2717",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
